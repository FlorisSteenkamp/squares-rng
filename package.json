{
    "name": "squares-rng",
    "description": "Squares: A Fast Counter-Based RNG",
    "version": "2.0.0",
    "author": {
        "name": "Floris Steenkamp"
    },
    "license": "MIT",
    "type": "module",
    "main": "./node/index.js",
    "exports": "./node/index.js",
    "engines": {
        "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
    },
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FlorisSteenkamp/squares-rng.git"
    },
    "bugs": {
        "url": "https://github.com/FlorisSteenkamp/squares-rng/issues"
    },
    "keywords": [
        "Fast",
        "Counter-Based",
        "RNG",
        "Random",
        "Number",
        "Generator",
        "Middle",
        "Square",
        "Method",
        "Bernard",
        "Widynski"
    ],
    "devDependencies": {
        "@assemblyscript/loader": "^0.19.20",
        "@types/chai": "^4.2.22",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.7",
        "assemblyscript": "^0.19.20",
        "chai": "^4.3.4",
        "circular-dependency-plugin": "^5.2.2",
        "mocha": "^9.1.3",
        "resolve-typescript-plugin": "^1.1.1",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2",
        "webpack": "^5.64.1",
        "webpack-cli": "^4.9.1"
    },
    "scripts": {
        "build": "npm run build-browser && npm run build-node",
        "build-wasm": "asc --runtime none --converge --optimizeLevel 3 --shrinkLevel 2 --binaryFile index.wasm --textFile index.wat assembly/index.ts",
        "build-node": "rimraf node && tsc",
        "build-browser": "rimraf browser && webpack",
        "test": "mocha",
        "coverage": "c8 mocha",
        "test-package": "npm pack"
    }
}
